    <%= form_for(officer) do |f| %>
      <%= render 'shared/error_messages', object: officer %>

      <%= f.label :last_name %>
      <%= f.text_field :last_name %>

      <%= f.label :first_name %>
      <%= f.text_field :first_name %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.submit "Add club", class: "btn btn-large btn-primary" %>
